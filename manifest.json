{
  "background": {
    "scripts": ["background.js"]
  },
  "description": "Add emojis to slack!",
  "icons": {
    "128": "slack.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "Slack Emoji Uploader",
  "permissions": ["contextMenus", "storage", "http://*/*", "https://*/*", "file://*/*"],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "version": "1.1.6",
  "minimum_chrome_version": "50.0.0.0"
}